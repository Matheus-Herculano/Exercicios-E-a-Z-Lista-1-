#include <stdio.h>
int main()

{
    int A, B, resultado;

    printf("Digite o valor de A: ");
    scanf("%d", &A);

    printf("Digite o valor de B: ");
    scanf("%d", &B);

    if (B != 0) {
        resultado = (A / B) * (A / B);
        printf("O quadrado da divisao de A por B eh: %d\n", resultado);
    } else {
        printf("Erro: Divisao por zero nao eh possivel.\n");
    }

    return 0;
}
