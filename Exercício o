#include <stdio.h>
void main()
{
    int a, b, c, d, p, s;

    printf("Escreva o valor da vari치vel a: ");
    scanf("%d", &a);

    printf("Escreva o valor da vari치vel b: ");
    scanf("%d", &b);

    printf("Escreva o valor da vari치vel c: ");
    scanf("%d", &c);

    printf("Escreva o valor da vari치vel d: ");
    scanf("%d", &d);

    p = a*c;
    s = b+d;

    printf("O resultado de a*c eh: %d \nO resultado de b+d eh: %d", p, s);
}
