#include <stdio.h>
int main()

{
    double base, indice, resultado;

    printf("Digite a base da raiz: ");
    scanf("%lf", &base);

    printf("Digite o indice da raiz: ");
    scanf("%lf", &indice);

    resultado = pow(base, 1.0 / indice);

    printf("A raiz de %.2f com indice %.2f eh %.2f\n", base, indice, resultado);

    return 0;
}
